{
  "version": 2,
  "builds": [
    {
      "src": "Server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/encarte(.*)",
      "dest": "Server.js"
    },
    {
      "src": "/api/cleanup",
      "dest": "Server.js"
    },
    {
      "src": "/(.*)",
      "dest": "Server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cleanup",
      "schedule": "12 15 * * *" 
    }
  ]
}